{
    "extends": "airbnb",
    "env": {
        "node": true,
        "browser": true,
        "es6":true
    },
    "rules":{
        "no-console" : "off",
        "quotes": [
            "error",
            "double"
        ],
        "comma-dangle":[
            "error",
            "never"
        ],
        "no-unsaved-vars":[
            "error",
            {
                "vars":"local",
                "args":"none"
            }
        ],
        "react/jsx-filename-extension": 0
    }
}